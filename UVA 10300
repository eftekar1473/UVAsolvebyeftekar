#include <stdio.h>
int main() {
int i,a,b,c,t,n;
     while(scanf("%d",&t)!=EOF){
         while(t--){
         scanf("%d",&n);
         int sum=0;
         for(i=1;i<=n;i++)
         {
           scanf("%d%d%d",&a,&b,&c);
           sum+=a*c;
         }
    printf("%d\n",sum);
     }
   }
   return 0;
}
